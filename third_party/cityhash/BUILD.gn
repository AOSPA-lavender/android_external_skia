import("../../gn/skia.gni")

source_set("cityhash") {
  sources = [
    "city.cc",
    "city.h",
  ]

  # Warnings are just noise if we're not maintaining the code.
  if (is_win) {
    cflags = [ "/w" ]
  } else {
    cflags = [ "-w" ]
  }
}
